		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"prism-svelte": "^0.5.0",
		"prismjs": "^1.27.0",
		"shiki": "0.10.1",
		"shiki-twoslash": "^3.0.2",
		"svelte": "^4.2.9",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.3.1",
		"typescript": "^5.0.0",
		"unist-util-visit": "^4.1.0",
		"vite": "^5.0.12",
		"vite-imagetools": "^4.0.12"
	}
